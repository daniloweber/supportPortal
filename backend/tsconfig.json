{
  "compilerOptions": {
    "target": "ESNext",
    "module": "NodeNext",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "moduleResolution": "NodeNext",
    "baseUrl": "./",
    "outDir": "./dist",
    "rootDir": "./",
    "paths": {

    },
    "strictPropertyInitialization": false,
    "allowSyntheticDefaultImports": true,
